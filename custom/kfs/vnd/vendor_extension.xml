  	<sql stripComments="true" endDelimiter="/">
  		<![CDATA[
        insert into PUR_VNDR_HDR_TX (VNDR_HDR_GNRTD_ID, OBJ_ID, VER_NBR, VNDR_W9_RCVD_DATE)  
          select VNDR_HDR_GNRTD_ID, sys_guid(), 1, VNDR_W9_RCVD_DATE from PUR_VNDR_HDR_T
  			/
  		]]>
   	</sql>
 	
 	</changeSet>
